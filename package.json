{
	"name": "vscode-git-cruise",
	"displayName": "Git Cruise",
	"description": "view changes between multiple commits",
	"keywords": [
		"git",
		"log",
		"diff",
		"cruise"
	],
	"version": "0.0.2",
	"license": "MIT",
	"icon": "assets/logo/1x.png",
	"repository": "https://github.com/raymon-sun/vscode-git-cruise",
	"publisher": "GuodongSun",
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:git-cruise.changes.view"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "changes-explorer",
					"title": "Changes Explorer",
					"icon": "assets/icons/files.svg"
				}
			]
		},
		"views": {
			"scm": [
				{
					"id": "git-cruise.changes",
					"name": "Git Cruise Changes",
					"icon": "assets/icons/files.svg",
					"contextualTitle": "Changes Explorer"
				}
			]
		},
		"commands": [
			{
				"category": "Git Cruise",
				"command": "git-cruise.changes.view",
				"title": "View Git Log",
				"icon": "$(list-flat)"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "git-cruise.changes.view",
					"when": "view =~ /git-cruise/",
					"group": "navigation@4"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./dist/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/lodash": "^4.14.176",
		"@types/mocha": "^9.0.0",
		"@types/node": "^16.11.6",
		"@types/react": "^17.0.34",
		"@types/react-dom": "^17.0.11",
		"@types/vscode": "^1.62.0",
		"@types/which": "^2.0.1",
		"@typescript-eslint/eslint-plugin": "^5.3.0",
		"@typescript-eslint/parser": "^5.3.0",
		"css-loader": "^6.5.1",
		"eslint": "^8.2.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"sass-loader": "^12.3.0",
		"style-loader": "^3.3.1",
		"ts-loader": "^9.2.6",
		"typescript": "^4.4.4",
		"typescript-plugin-css-modules": "^3.4.0",
		"vscode-test": "^1.6.1",
		"webpack": "^5.62.1",
		"webpack-cli": "^4.9.1"
	},
	"dependencies": {
		"@use-gesture/react": "^10.1.3",
		"classnames": "^2.3.1",
		"dayjs": "^1.10.7",
		"iconv-lite-umd": "^0.6.8",
		"inversify": "^5.1.1",
		"lodash": "^4.17.21",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-select": "^5.2.1",
		"react-virtual": "^2.8.2",
		"reflect-metadata": "^0.1.13",
		"simple-git": "^2.47.0",
		"vscode-nls": "^5.0.0",
		"which": "^2.0.2"
	}
}
