{
	"name": "vscode-git-cruise",
	"displayName": "Git Cruise",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:git-cruise.changes.view"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "changes-explorer",
					"title": "Changes Explorer",
					"icon": "assets/icons/files.svg"
				}
			]
		},
		"views": {
			"scm": [
				{
					"id": "git-cruise.changes",
					"name": "Git Cruise Changes",
					"icon": "assets/icons/files.svg",
					"contextualTitle": "Changes Explorer"
				}
			]
		},
		"commands": [
			{
				"category": "Git Cruise",
				"command": "git-cruise.changes.view",
				"title": "View Git Log",
				"icon": "$(list-flat)"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "git-cruise.changes.view",
					"when": "view =~ /git-cruise/",
					"group": "navigation@4"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./dist/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/lodash": "^4.14.172",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/react": "^17.0.9",
		"@types/react-dom": "^17.0.6",
		"@types/vscode": "^1.56.0",
		"@types/which": "^2.0.1",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"css-loader": "^5.2.6",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"sass-loader": "^12.0.0",
		"style-loader": "^2.0.0",
		"ts-loader": "^8.0.14",
		"typescript": "^4.1.3",
		"typescript-plugin-css-modules": "^3.4.0",
		"vscode-test": "^1.5.0",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	},
	"dependencies": {
		"@use-gesture/react": "^10.0.0-beta.22",
		"classnames": "^2.3.1",
		"dayjs": "^1.10.6",
		"iconv-lite-umd": "^0.6.8",
		"inversify": "^5.1.1",
		"lodash": "^4.17.21",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-select": "^5.0.0",
		"react-virtual": "^2.8.2",
		"reflect-metadata": "^0.1.13",
		"simple-git": "^2.46.0",
		"vscode-nls": "^5.0.0",
		"which": "^2.0.2"
	}
}
